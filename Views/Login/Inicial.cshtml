                <div class="hero-slides-nav bg-img" style="background-image: url('https://images7.alphacoders.com/431/431286.jpg'); "></div>
            </div>
            <!--  grande-->
            <div class="single-hero-slides bg-img" style="background-image: url('https://images7.alphacoders.com/431/431286.jpg'); ">
                <div class="container h-100">
                    <div class="row h-100 align-items-center">
                        <div class="col-11 col-md-6 col-lg-4">
                            <div class="hero-content">
                                <h2>Bugatti</h2>
