@model IEnumerable<ProyectoNexus.ReferenciaServicioAuto.CAuto>

@{
    ViewBag.Title = "Auto";
}

<h2>Auto</h2>

<p>
    @Html.ActionLink("Agregar Auto", "AgregarAutos")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AUTCANT)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AUTCOD)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AUTMARC)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AUTMODEL)
        </th>
        <th>
