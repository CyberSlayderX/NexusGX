            @Html.DisplayFor(modelItem => item.AUTCANT)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AUTCOD)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AUTMARC)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AUTMODEL)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AUTPRECI)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GERECOD)
        </td>
        <td>
            @Html.ActionLink("Editar", "ModificarAutos", new { id=item.AUTCOD  }) |
            @Html.ActionLink("Details", "Details", new { id=item.AUTCOD  }) |
